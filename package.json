  "module": "esm/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thipages/js-crud-api.git"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "rollup-plugin-babel": "^4.4.0",
